{
  "name": "mini-prosperia",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p .",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write .",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down",
    "test:health": "node -e \"require('http').get('http://localhost:3000/health',r=>{console.log('Status',r.statusCode)})\""
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pdf2pic": "^3.2.0",
    "pino": "^9.1.0",
    "sharp": "^0.34.3",
    "tesseract.js": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.12.12",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
